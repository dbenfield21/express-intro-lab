<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Express Intro Lab</title>
</head>
<body>
  <h1>Grades</h1>
  <% if(error) { %>
    <h2> <%= error %>  </h2>
  <%  } %>
  <ul>
    <% grades.forEach(function(grade) { %>
      <li>
        <%= grade.name %>
        -
        <%= grade.pass ? "Pass" : "Fail" %>
      </li>
    <% }) %>
  </ul>
</body>
</html>